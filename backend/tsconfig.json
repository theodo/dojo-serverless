{
  "extends": "./tsconfig.paths.json",
  "compilerOptions": {
    "lib": [
      "es2019"
    ],
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "target": "es2019",
    "outDir": "lib",
    "strict": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules"
  ]
}